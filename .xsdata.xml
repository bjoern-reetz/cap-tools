<?xml version="1.0" encoding="UTF-8"?>
<Config xmlns="http://pypi.org/project/xsdata" version="24.4">
  <Output maxLineLength="79" subscriptableTypes="true" unionType="false">
    <Package>cap_tools.models</Package>
    <Format repr="true" eq="true" order="false" unsafeHash="false" frozen="false" slots="true" kwOnly="true">dataclasses</Format>
    <Structure>single-package</Structure>
    <DocstringStyle>reStructuredText</DocstringStyle>
    <RelativeImports>false</RelativeImports>
    <CompoundFields defaultName="choice" useSubstitutionGroups="false" forceDefaultName="false" maxNameParts="3">true</CompoundFields>
    <WrapperFields>false</WrapperFields>
    <PostponedAnnotations>false</PostponedAnnotations>
    <UnnestClasses>true</UnnestClasses>
    <IgnorePatterns>true</IgnorePatterns>
    <IncludeHeader>false</IncludeHeader>
  </Output>
  <Conventions>
    <ClassName case="pascalCase" safePrefix="type"/>
    <FieldName case="snakeCase" safePrefix="value"/>
    <ConstantName case="screamingSnakeCase" safePrefix="value"/>
    <ModuleName case="snakeCase" safePrefix="mod"/>
    <PackageName case="snakeCase" safePrefix="pkg"/>
  </Conventions>
  <Substitutions>
    <Substitution type="package" search="http://www.w3.org/2001/XMLSchema" replace="xs"/>
    <Substitution type="package" search="http://www.w3.org/XML/1998/namespace" replace="xml"/>
    <Substitution type="package" search="http://www.w3.org/2001/XMLSchema-instance" replace="xsi"/>
    <Substitution type="package" search="http://www.w3.org/1998/Math/MathML" replace="mathml3"/>
    <Substitution type="package" search="http://www.w3.org/1999/xlink" replace="xlink"/>
    <Substitution type="package" search="http://www.w3.org/1999/xhtml" replace="xhtml"/>
    <Substitution type="package" search="http://schemas.xmlsoap.org/wsdl/soap/" replace="soap"/>
    <Substitution type="package" search="http://schemas.xmlsoap.org/wsdl/soap12/" replace="soap12"/>
    <Substitution type="package" search="http://schemas.xmlsoap.org/soap/envelope/" replace="soapenv"/>
    <Substitution type="class" search="(.*)Class$" replace="\1Type"/>
    <Substitution type="class" search="Alert(.+)$" replace="\1"/>
    <Substitution type="class" search="Info(.+)$" replace="\1"/>
    <Substitution type="class" search="Area(.+)$" replace="\1"/>
    <Substitution type="field" search="^code$" replace="codes"/>
    <Substitution type="field" search="^info$" replace="infos"/>
    <Substitution type="field" search="^signature$" replace="signatures"/>
    <Substitution type="field" search="^category$" replace="categories"/>
    <Substitution type="field" search="^response_type$" replace="response_types"/>
    <Substitution type="field" search="^event_code$" replace="event_codes"/>
    <Substitution type="field" search="^parameter$" replace="parameters"/>
    <Substitution type="field" search="^resource$" replace="resources"/>
    <Substitution type="field" search="^area$" replace="areas"/>
    <Substitution type="field" search="^polygon$" replace="polygons"/>
    <Substitution type="field" search="^circle$" replace="circles"/>
    <Substitution type="field" search="^geocode$" replace="geocodes"/>
    <Substitution type="field" search=".*xmldsig_element$" replace="_signatures"/>
  </Substitutions>
  <Extensions/>
</Config>
